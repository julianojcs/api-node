  "version": "1.0.0",
  "description": "Typescript with NodeJs",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --inspect --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run"
  },
  "keywords": [
    "node.js",
    "typescript"
  ],
