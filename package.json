    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.8",
    "multer": "^1.4.2",
    "sqlite3": "^5.0.0"
  }
